page{
  background-color: #eee;
}
.percent_box{
  padding: 0 35rpx;
  box-sizing: border-box;
  background-color: #ffffff;
  display: flex;
  justify-content: flex-start ;
  margin: 15rpx 0;
}
.shops_price{
  font-size: 35rpx;
  color: #333;
  width: 35%;
}
.shops_price>text{
  display: block;
  height: 100rpx;
  line-height: 100rpx;
}
.check{
  width: 65%;
  font-size: 30rpx;
  color: #666;
}
.check>view{
  width: 100%;
  height: 100rpx;
  line-height: 100rpx;
  display: flex;
  justify-content: flex-start;
}
.percent_inp{
  width: 100rpx;
  height: 100rpx;
  line-height: 100rpx;
  text-align: center;
  font-size: 35rpx;
}
/*修改默认样式  */
checkbox .wx-checkbox-input{
   border-radius: 8rpx;
   width: 35rpx;
   height: 35rpx;
   margin: 0 15rpx 4rpx 0;
}
checkbox .wx-checkbox-input.wx-checkbox-input-checked{
   border-color: #cbe9fc;
   background: #1e9cf9;
}
checkbox .wx-checkbox-input.wx-checkbox-input-checked::before{
   border-radius: 50%;
   width: 35rpx;
   height: 35rpx;
   line-height: 35rpx;
   text-align: center;
   font-size:30rpx;
   color:#fff;
   background: transparent;
   transform:translate(-50%, -50%) scale(1);
   -webkit-transform:translate(-50%, -50%) scale(1);
}
/*店铺信息  */
.shops_infor{
  width: 100%;
  background-color: #fff;
  padding: 0 35rpx;
  box-sizing: border-box;
}
.expert{
  height: 100rpx;
  line-height: 100rpx;
  font-size: 35rpx;
  color: #333;
  display: flex;
  justify-content: space-between;
}
.font_style{
  color: #8e8e8e;
  font-size: 30rpx;
}
.border_line{
  border-bottom: 3rpx solid #eee;
}
/*保存  */
.keep{
  width: 100%;
  height: 100rpx;
  line-height: 100rpx;
  text-align: center;
  color: #fff;
  font-size: 40rpx;
  background-color: #219dfa;
  position: fixed;
  bottom: 0;
  left: 0;
}
