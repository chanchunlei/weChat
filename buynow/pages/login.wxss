.container{
  padding: 0 45rpx;
  box-sizing: border-box;
}

.logos{
  width: 150rpx;
  height: 150rpx;
  margin: 0 auto;
  padding-top: 80rpx;
}
.logos>image{
  width: 150rpx;
  height: 150rpx;
}
.form_box{
  width: 100%;
  margin: 50rpx 0;
}
.inp_box{
  width: 100%;
  padding: 40rpx;
  box-sizing: border-box;
  background-color: #f5f5f5; 
  font-size: 38rpx;
  color: #666666;
  border: 1px solid #eee;
  border-top: 0;
  border-radius: 5px;
  box-shadow: 0px 2px 5px 2px rgba(0, 0, 0, 0.15);
}
.inp_box>label>input{
  width: 100%;
  height: 70rpx;
  line-height: 70rpx;
  background-color: #fcfcfc;
	border: 1px solid #eee;
	border-radius: 3px;
  margin: 15rpx 0;
  box-shadow: 2rpx -2rpx 5rpx 1rpx rgba(0, 0, 0, 0.15);
}
.btn{
  margin-top: 50rpx;
}
.btn>button{
  font-size: 38rpx;
  color: #fff;
  width: 100%;
  background-color: #219fea; 
}
/*登录状态  */
.animation_box{
  position: absolute;
  left: 0;
  color: #999;
  font-size: 35rpx;
  width: 100%;
  text-align: center;
  top: -300rpx;
}
.animation{
  background-color: #fff;
  border: 1px solid #eee;
  display: inline-block;
  padding: 7rpx;
  border-radius: 10rpx;
  box-shadow: 10rpx 10rpx 10rpx rgba(0, 0, 0, 0.15);
}
.move{
  transform: translateY(305rpx);
  transition: transform 0.3s;
}