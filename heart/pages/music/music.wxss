page,
.page,
.page__bd{
    height: 100%;
}
.page__bd{
    padding-bottom: 0;
}
.weui-tab__content{
    width:100%; 
    display: flex; 
    flex-flow: row wrap;
    justify-content: space-around;
}
.weui-navbar{
  background-color: #fff;
}
.navs{
    width: 100%;
    font-size: 37rpx;
    color: #666;
    box-sizing: border-box;
    padding: 20rpx 30rpx;
}
.Imgs{
    width: 47%; 
}
.Imgs>navigator{
    width: 100%;
    height: 350rpx;
}

.weui-search-bar{
    width: 100%;
}
.searchbar-reault{
    width: 100%;
}
.search_input{
  display: block
}
.weui-cells{
  margin-top: 0;
}

.model_black{
  position: fixed;
  width: 100%;
  height: 100%;
  background: rgba(0, 0, 0, 0.3);
  z-index: 2;
  top: 0;
  left: 0;
  display: none;
}
.model_massage{
  position: fixed;
  bottom: -430rpx;
  left: 0;
  transition: all 0.3s;
  box-shadow: 0 0 5px rgba(0, 0, 0, 0.2);
  border-radius: 3px 3px 0 0;
  z-index: 3;
  width: 100%;
  height: 220rpx;
  background-color: #fff;
  padding-top: 120rpx;
}
.model_img{
  position: absolute;
  top: -80rpx;
  left: 50%;
  margin-left: -85rpx;
  background-color: #fff;
  width: 170rpx;
  height: 170rpx;
  box-shadow: 0 0 10px rgba(0, 0, 0, 0.5);
  border-radius: 50% 50%;
  padding: 6rpx;
}
.model_img>image{
  width: 100%;
  height: 100%;
  border-radius: 50% 50%;
}
.connect{
  width: 90%;
  margin: 0 5%;
  height: 100rpx;
  line-height: 100rpx;
  border-top: 1px solid rgba(0, 0, 0, 0.2);
  text-align: center;
  color: #333;
}
.model_massage>.connect:last-child{
  border-bottom: 1px solid rgba(0, 0, 0, 0.2);
}