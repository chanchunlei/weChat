/**index.wxss**/
.contain{
  height: 100%;
}
.searchbar-result{
    margin-top: 0;
    font-size: 14px;
}
.searchbar-result:before{
    display: none;
}
.weui-cell{
    padding: 12px 0;
    display: block;
}
.weui-cell__bd{
  font-size: 33rpx;
  color: #666;
  text-align: center;
}
.loopImg>swiper{
  height: 450rpx;
}
.image_to{
  width: 100%;
  height: 450rpx;
}
.scenery{
    padding: 20rpx 20rpx;
}
.scenery>text{
  display: block;
  box-sizing: border-box;
  padding: 10rpx;
  width: 100%;
  color: #666;
  border-bottom: 1rpx solid rgba(0, 0, 0, 0.2);
}
.conent{
  width: 100%;
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
  justify-content: space-around;
}
.img{
  width: 100%;
  text-align: center;
  box-shadow: 0 0 3px rgba(0, 0, 0, 0.2);
  height: 450rpx;
  margin-top: 20rpx;
  background: rgba(232, 232, 232, 0.1);
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
}
.img>navigator{
  display: inline-block;
  width: 100%;
}
.img>.imgs_a{
  height: 450rpx;
}

.weui-search-bar__cancel-btn{
  color: #fac;
  font-size: 35rpx;
}
.model_black{
  position: fixed;
  width: 100%;
  height: 100%;
  background: rgba(0, 0, 0, 0.3);
  z-index: 2;
  top: 0;
  left: 0;
  display: none;
}
.model_massage{
  position: fixed;
  bottom: -430rpx;
  left: 0;
  transition: all 0.3s;
  box-shadow: 0 0 5px rgba(0, 0, 0, 0.2);
  border-radius: 3px 3px 0 0;
  z-index: 3;
  width: 100%;
  height: 220rpx;
  background-color: #fff;
  padding-top: 120rpx;
}
.model_img{
  position: absolute;
  top: -80rpx;
  left: 50%;
  margin-left: -85rpx;
  background-color: #fff;
  width: 170rpx;
  height: 170rpx;
  box-shadow: 0 0 10px rgba(0, 0, 0, 0.5);
  border-radius: 50% 50%;
  padding: 6rpx;
}
.model_img>image{
  width: 100%;
  height: 100%;
  border-radius: 50% 50%;
}
.connect{
  width: 90%;
  margin: 0 5%;
  height: 100rpx;
  line-height: 100rpx;
  border-top: 1px solid rgba(0, 0, 0, 0.2);
  text-align: center;
  color: #333;
}
.model_massage>.connect:last-child{
  border-bottom: 1px solid rgba(0, 0, 0, 0.2);
}


